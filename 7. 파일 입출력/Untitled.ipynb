    "from abc import *\n",
    "class Car(metaclass=ABCMeta):\n",
    "    @abstractmethod\n",
    "    def run(self):\n",
    "        pass\n",
    "\n",
    "class Electric:\n",
    "    def charge(self):\n",
    "        print(\"충전 한다.\");\n",
    "\n",
    "class Model3(Car, Electric):\n",
    "    def run(self):\n",
    "        print(\"모델3가 달린다\")\n",
    "    \n",
    "m = Model3()\n",
    "m.charge()\n",
    "m.run()"
   ]
